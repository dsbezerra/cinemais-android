<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="AutoSlideViewPager">
        <attr format="integer" name="slideInterval"/>
    </declare-styleable>

    <declare-styleable name="EmptyView">
        <attr format="reference" name="emptyIcon"/>
        <attr format="reference" name="emptyIconTint"/>
        <attr format="string" name="emptyMessage"/>
    </declare-styleable>

    <declare-styleable name="ErrorView">
        <attr format="reference" name="errorIcon"/>
        <attr format="reference" name="errorIconTint"/>
        <attr format="string" name="errorTitle"/>
        <attr format="string" name="errorMessage"/>
    </declare-styleable>

    <declare-styleable name="MovieInfoView">
        <attr format="string" name="miLabel"/>
        <attr format="string" name="miContent" />
    </declare-styleable>

    <declare-styleable name="CinemaActionView">
        <attr format="string" name="caLabel"/>
        <attr format="reference" name="caIcon" />
        <attr format="reference" name="caColor" />
    </declare-styleable>

    <declare-styleable name="SessionFilterView">
        <attr format="string" name="filterId"/>
        <attr format="string" name="filterName"/>
    </declare-styleable>

</resources>